{
  "manifest_version": 3,
  "name": "Smart Content Filter",
  "version": "1.0",
  "description": "A basic Chrome extension with popup",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Smart Content Filter"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://openrouter.ai/*"],
  "web_accessible_resources": [
    {
      "resources": ["config.js"],
      "matches": ["<all_urls>"]
    }
  ]
}